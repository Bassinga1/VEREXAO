<!DOCTYPE html>

<title>Contact</title>

{% block body %}
    <h1>Contact</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ contact.id }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ contact.email }}</td>
            </tr>
            <tr>
                <th>Sujet</th>
                <td>{{ contact.sujet }}</td>
            </tr>
            <tr>
                <th>Message</th>
                <td>{{ contact.message }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_admin_contract_index') }}">back to list</a>

    <a href="{{ path('app_admin_contract_edit', {'id': contact.id}) }}">edit</a>

    {{ include('admin_contract/_delete_form.html.twig') }}
{% endblock %}
